{
  "name": "trace-pretty",
  "version": "0.1.0",
  "description": "A tool to make JavaScript/TypeScript stack traces readable, elegant, and actionable",
  "main": "dist/index.js",
  "bin": {
    "trace-pretty": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:demo": "ts-node examples/test-stack-trace.ts",
    "test:real": "ts-node examples/test-real-error.ts",
    "test:elegant": "ts-node examples/test-elegant-formatting.ts",
    "test:clean": "ts-node examples/test-clean-format.ts",
    "test:target": "ts-node examples/test-target-format.ts",
    "test:real-example": "ts-node examples/test-real-example.ts",
    "test:advanced": "ts-node examples/test-advanced-format.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "stack-trace",
    "error-handling",
    "debugging",
    "typescript",
    "node",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "source-map": "^0.7.4",
    "yargs": "^17.7.2",
    "highlight.js": "^11.9.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
